#set ($isUnnotified = ("$!{request.getParameter('type')}" == 'unnotified'))
{{html clean = false}}
&lt;div class="multitab-selector-tabs"&gt;
  &lt;dd class="#if($isUnnotified)active#end"&gt;&lt;a href="$xwiki.getURL('data.MyMatchesResults', 'view', 'type=unnotified')"&gt;$services.localization.render('phenotips.myMatches.unnotifiedMatches') &lt;/a&gt;&lt;/dd&gt;
