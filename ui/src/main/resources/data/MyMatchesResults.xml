&lt;/div&gt;
{{/html}}

#if($isUnnotified)
  {{include reference="MatchingNotification.UnnotifiedTable" /}}
#else
  {{include reference="MatchingNotification.NotifiedTable" /}}
#end
{{/velocity}}</content>
